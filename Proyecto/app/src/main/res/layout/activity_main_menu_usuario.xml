<?xml version="1.0" encoding="utf-8"?>
<androidx.coordinatorlayout.widget.CoordinatorLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainMenuUsuario">




    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/constraintLayout"
        android:layout_width="wrap_content"

        android:layout_height="wrap_content">

    </androidx.constraintlayout.widget.ConstraintLayout>
    <com.google.android.material.bottomnavigation.BottomNavigationView
        android:id="@+id/botonNavegacion"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_gravity="bottom"
        android:background="@color/colorPrimary"
        app:itemTextColor="#FFFFFF"
        app:itemIconTint="#FFFFFF"
        app:menu="@menu/main_menu_usuario">
    </com.google.android.material.bottomnavigation.BottomNavigationView>



    <ImageView
        android:id="@+id/imageView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="330dp"
        android:layout_marginTop="40dp"
        android:scrollbarSize="80dp"
        android:background="@drawable/borde"
        app:srcCompat="@drawable/ic_person" />

    <TextView
        android:id="@+id/txtHola"
        android:layout_width="60dp"
        android:layout_height="40dp"
        android:layout_marginStart="30dp"
        android:layout_marginTop="50dp"
        android:layout_marginEnd="151dp"
        android:layout_marginBottom="642dp"
        android:text="Hola,"
        android:textSize="20dp" />
    <TextView
        android:id="@+id/txtRecibe"
        android:layout_width="230dp"
        android:layout_height="40dp"
        android:layout_marginStart="80dp"
        android:layout_marginTop="50dp"
        android:layout_marginEnd="151dp"
        android:layout_marginBottom="642dp"
        android:textSize="20dp" />

    <EditText
        android:id="@+id/editTextTextMultiLine"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="30dp"
        android:layout_marginTop="130dp"
        android:layout_marginEnd="50dp"
        android:ems="50"
        android:gravity="start|top"
        android:inputType="textMultiLine"
        android:text="!ENCUENTRA ESTACIONAMIENTO EN CUALQUIER LUGAR !"
        android:textAlignment="center"
        android:textColor="@color/colorPrimary"
        android:textStyle="bold" />

    <RatingBar
        android:id="@+id/ratingBar"
        android:layout_width="53dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="30dp"
        android:layout_marginTop="240dp"
        android:layout_marginEnd="50dp"
        android:focusable="auto"
        android:textAlignment="center" />

    <TextView
        android:id="@+id/txtFav"
        android:layout_width="230dp"
        android:layout_height="40dp"
        android:layout_marginStart="90dp"
        android:layout_marginTop="250dp"
        android:layout_marginEnd="151dp"
        android:layout_marginBottom="642dp"
        android:textSize="20dp"
        android:text="Favoritos"/>

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="53dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="30dp"
        android:layout_marginTop="310dp"
        android:layout_marginEnd="50dp"
        app:srcCompat="@drawable/trademark_parking_location" />

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="78dp"
        android:layout_height="45dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="390dp"
        android:layout_marginEnd="50dp"
        app:srcCompat="@android:drawable/btn_dialog" />

    <TextView
        android:id="@+id/txtFav3"
        android:layout_width="230dp"
        android:layout_height="40dp"
        android:layout_marginStart="90dp"
        android:layout_marginTop="400dp"
        android:layout_marginEnd="151dp"
        android:layout_marginBottom="642dp"
        android:text="Cerrar SesiÃ³n"
        android:textSize="20dp" />

    <TextView
        android:id="@+id/txtFav2"
        android:layout_width="230dp"
        android:layout_height="40dp"
        android:layout_marginStart="90dp"
        android:layout_marginTop="330dp"
        android:layout_marginEnd="151dp"
        android:layout_marginBottom="642dp"
        android:text="Buscar Estacionamiento"
        android:textSize="20dp" />


</androidx.coordinatorlayout.widget.CoordinatorLayout>